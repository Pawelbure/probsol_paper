@article{Lengfelder.2002,
 abstract = {Nevertheless, our Olympians gave us many valuable ideas on how to ameliorate the situation. Their suggestions include: (a) broaden the goals of the academic programs, de-emphasizing the competition aspect; (b) encourage girls to take more math and science courses and facilitate their interests in these subjects; (c) positively recognize females' achievement in mathematics and science; (d) hold high expectations for both male and female students in school settings and honor their talents equally; (e) make the qualifying exams for the Olympiad accessible at the grass root level; (f) inform parents about the impact of differential child-rearing practices on their children, particularly their daughters; (g) inform teachers about the differences between their male and female students, and recognizing these differences let girls learn in their own way, (h) to provide academic programs for young women that will be supportive; (i) reform the format of the Olympiad program, including team work, by allowing group collaboration; (j) lobby the media to discourage advertising promoting gender bias messages and visual stereotypes; and (k) re-examine the structure of mathematics and science in general, redefining what accounts for the best mathematics and science spirit, and what are the alternative ways that these subjects can be done.},
 author = {Lengfelder, A. and Heller, K. A.},
 year = {2002},
 title = {German Olympiad Studies: Findings from a Retrospective Evaluation and from In-Depth Interviews. Where Have all the Gifted Females Gone?},
 pages = {86--92},
