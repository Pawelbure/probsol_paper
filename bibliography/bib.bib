@article{Meeus.2010,
 abstract = {This study examined identity development in a 5-wave study of 923 early-to-middle and 390 middle-to-late adolescents thereby covering the ages of 12-20. Systematic evidence for identity progression was found: The number of diffusions, moratoriums, and searching moratoriums (a newly obtained status) decreased, whereas the representation of the high-commitment statuses (2 variants of a [fore]closed identity: {\textquotedbl}early closure{\textquotedbl} and {\textquotedbl}closure,{\textquotedbl} and achievement) increased. We also found support for the individual difference perspective: 63{\%} of the adolescents remained in the same identity status across the 5 waves. Identity progression was characterized by 7 transitions: diffusion$\rightarrow$moratorium, diffusion$\rightarrow$early closure, moratorium$\rightarrow$closure, moratorium$\rightarrow$achievement, searching moratorium$\rightarrow$closure, searching moratorium$\rightarrow$achievement, and early closure$\rightarrow$achievement.},
 author = {Meeus, Wim and {van de Schoot}, Rens and Keijsers, Loes and Schwartz, Seth J. and Branje, Susan},
 year = {2010},
 title = {On the progression and stability of adolescent identity formation: A five-wave longitudinal study in early-to-middle and middle-to-late adolescence},
 keywords = {Adolescent;Adolescent Development;Adult;Child;Female;Humans;Longitudinal Studies;Male;Models, Psychological;Self Concept;Young Adult},
 pages = {1565--1581},
