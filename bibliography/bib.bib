@article{Archer.,
 abstract = {Our approach draws on theorizations of identity as nonessentialized, fluid, contested, and

produced through discourse (Burman {\&} Parker, 1993; Gee, 1996).





Moreover, identities are social products, produced

within and through discourse and social inequalities---identities are ``real fictions'' that are

produced and constructed through social life and relations of power (Foucault, 1978;Weeks,

1981).



We draw upon the work of Judith Butler to foreground an analysis of gender within girls'

science aspirations. Butler (1990) conceptualizes social identities, such as gender, as performative.

In other words, gender is not the ``result'' of a person's sex---it does not emanate

``naturally'' from particular (sexed, racialized, classed) bodies---but is produced through

discursive and bodily ``acts.'' Gender (and thus identity) is, therefore, not something you

``are'' or ``have'' but rather is something that you ``do'' (perform) and continually redo.





As Butler argues, ``. . . the regulatory practices that govern gender also govern

culturally intelligible notions of identity,'' such that ``the `coherence' and `continuity' of

`the person' are not logical or analytic features of personhood, but rather, socially instituted

and maintained norms of intelligibility'' (Butler, 1990, p. 23).},
 author = {Archer, Louise}
}


